 <!-- currency, gems, seed, tomato, watermelon, potato, pumpkin, corn, carrot, cabbage -->
 <!DOCTYPE html>

 <html lang="en" dir="ltr">
   <head>
     <link rel="stylesheet" href="stylesheets/login.css">
     <script type="text/javascript" src="js/login.js"></script>
     <meta charset="utf-8">

   </head>

         <body style="margin: 0;">

 <header id="site-header">
 </header>
         <header id="mainheader">
                 <h3 id = "date"></h3>
                 <h2>Inventory</h2>

                 <table>
                   <tr>
                     <th>Username</th>
                     <th>Coins</th>
                     <th>Gems</th>
                     <th>Seeds</th>
                     <th>Tomato</th>
                     <th>Potato</th>
                     <th>Pumpkin</th>
                     <th>Corn</th>
                     <th>Cabbage</th>
                     <th>Carrot</th>
                     <th>Watermelon</th>
                   </tr>

                   <% rows.forEach((user_name) => { %>
                   <tr>
                     <td><%= user_name.username %></td>
                     <td><%= user_name.coin %></td>
                     <td><%= user_name.gem %></td>
                     <td><%= user_name.seed %></td>
                     <td><%= user_name.tomato %></td>
                     <td><%= user_name.potato %></td>
                     <td><%= user_name.pumpkin %></td>
                     <td><%= user_name.corn %></td>
                     <td><%= user_name.cabbage %></td>
                     <td><%= user_name.carrot %></td>
                     <td><%= user_name.watermelon %></td>
                   </tr>
                   <% }); %>
                   </table>
             </header>




             <script>
             function DateFunction(){
               var curr_date = new Date();
               document.getElementById("date").innerHTML = curr_date;
             }
             </script>

             <script>
             setInterval(DateFunction, 1000);
             </script>


           <footer id="site-footer">
           </footer>

         </body>
       </html>
