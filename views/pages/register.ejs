<!DOCTYPE html>

<html lang="en" dir="ltr">
  <head>
    <link rel="stylesheet" href="stylesheets/login.css">
    <script type="text/javascript" src="js/login.js"></script>
    <meta charset="utf-8">

  </head>

        <body style="margin: 0;">

          <header id="site-header">
          </header>

        <header id="mainheader">
                <h1>Register</h1>

          <div id="logindiv">

            <!-- change the form below to add data to server-->
            <form action="/login" method="post">

              <label class="logintext">Username</label><br>
                  <input type="text" name="username" class="tboxes"><br><br>
                <label class="logintext">Password</label><br>
                  <input type="password" id="tmName" name="password" class="tboxes"><br><br>

                <button type="submit" class="finishReg">Finish</button>

            </form>
          </div>

            </header>

          <footer id="site-footer">
          </footer>

        </body>
      </html>
